{
  "name": "vue-urql-nexus",
  "version": "0.0.0",
  "scripts": {
    "server": "yarn ts-node graphql/server.ts",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "codegen": "graphql-codegen --config ${PWD}/graphql-codegen.yml",
    "build-schema": "ts-node --transpile-only graphql/schema.ts"
  },
  "dependencies": {
    "@urql/vue": "^0.4.3",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^2.0.2",
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/typed-document-node": "^1.18.9",
    "@graphql-codegen/typescript": "^1.22.4",
    "@graphql-codegen/typescript-operations": "^1.18.3",
    "@graphql-typed-document-node/core": "^3.1.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@vitejs/plugin-vue": "^1.3.0",
    "@vue/compiler-sfc": "^3.0.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.1",
    "nexus": "^1.1.0",
    "nexus-decorators": "^0.2.0",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.2",
    "vite": "^2.4.4",
    "vue-tsc": "^0.2.2"
  }
}
